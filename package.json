{
	"name": "advancedreplacer",
	"displayName": "advanced-replacer",
	"publisher": "truman",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "advancedreplacer.runpycommand",
				"title": "RunPyCommand"
			},
			{
				"command": "advancedreplacer.openpyfile",
				"title": "OpenPyFile"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"python-shell": "^3.0.1"
	},
	"repository": {
		"type": "git",
		"url": "https://gitee.com/swordjjjkkk/runpycommand.git"
	},
	"bugs": {
		"url": "https://gitee.com/swordjjjkkk/runpycommand/issues"
	}
}
